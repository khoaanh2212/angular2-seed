{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "test-karma": "karma start karma.conf.js",
    "test": "mocha tests",
    "api-stub": "json-server --watch tests/resources/heroes.json --port 3004",
    "clean": "rm -rf dist",
    "build-prod": "npm run clean && mkdir -p dist/lib && mkdir -p dist/min && tsc --module system && gulp build",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "angular2-in-memory-web-api": "0.0.20",
    "axios": "^0.14.0",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "gulp-sourcemaps": "^1.6.0",
    "ramda": "^0.22.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23"
  },
  "peerDependencies": {
    "@angular/common": "2.0.0",
    "@angular/core": "2.0.0"
  },
  "devDependencies": {
    "ts-node": "^1.6.0",
    "brfs": "^1.4.3",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "concurrently": "^2.0.0",
    "del": "^2.1.0",
    "gulp": "^3.9.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.8.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.2",
    "karma-browserify": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^1.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-tsc-preprocessor": "0.0.6",
    "karma-typescript-preprocessor": "^0.2.1",
    "karma-typescript-preprocessor2": "^1.2.1",
    "lite-server": "^2.2.0",
    "mocha": "^3.0.2",
    "pump": "^1.0.1",
    "remap-istanbul": "^0.6.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "systemjs": "^0.19.27",
